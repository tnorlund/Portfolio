{
  "mcpServers": {
    "nextjs-project-state": {
      "command": "node",
      "args": [
        "mcp-server.js"
      ],
      "cwd": "portfolio",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "python-receipts": {
      "command": "python",
      "args": [
        "mcp_server.py"
      ],
      "cwd": ".",
      "env": {
        "OPENAI_API_KEY": "",
        "PINECONE_API_KEY": "",
        "AWS_ACCESS_KEY_ID": "",
        "AWS_SECRET_ACCESS_KEY": "",
        "GOOGLE_PLACES_API_KEY": ""
      }
    }
  }
}
